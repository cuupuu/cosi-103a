// routes/recipe_uprate.js

module.exports = function(app, recipes) {
    app.post('/api/recipes', (req, res) => {
      const recipe = req.body; // Assuming the body contains a recipe object
      recipe.id = recipes.length + 1; // Assign a simple unique ID
      recipes.push(recipe);
      res.status(201).send(recipe);
    });
  };
  